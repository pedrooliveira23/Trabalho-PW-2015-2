<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <filter>
  	<filter-name>SegurancaFilter</filter-name>
  	<display-name>SegurancaFilter</display-name>
  	<description></description>
  	<filter-class>servlet.login.SegurancaFilter</filter-class>
  </filter>
  <filter-mapping>
  	<filter-name>SegurancaFilter</filter-name>
  	<url-pattern>/SegurancaFilter</url-pattern>
  </filter-mapping>
  <servlet>
  	<servlet-name>LoginServlet</servlet-name>
  	<display-name>LoginServlet</display-name>
  	<description></description>
  	<servlet-class>servlets.login.LoginServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<display-name>LogoutServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.login.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>IndexServlet</servlet-name>
  	<display-name>IndexServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.paginas.IndexServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>NovaInstituicaoServlet</servlet-name>
  	<display-name>NovaInstituicaoServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.paginas.NovaInstituicaoServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PesquisarServlet</servlet-name>
  	<display-name>PesquisarServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.paginas.PesquisarServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RedirServlet</servlet-name>
  	<display-name>RedirServlet</display-name>
  	<description></description>
  	<servlet-class>servlet.paginas.RedirServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginServlet</servlet-name>
  	<url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutServlet</servlet-name>
  	<url-pattern>/LogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>IndexServlet</servlet-name>
  	<url-pattern>/IndexServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>NovaInstituicaoServlet</servlet-name>
  	<url-pattern>/NovaInstituicaoServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PesquisarServlet</servlet-name>
  	<url-pattern>/PesquisarServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RedirServlet</servlet-name>
  	<url-pattern>/RedirServlet</url-pattern>
  </servlet-mapping>
  <error-page>
  	<error-code>500</error-code>
  	
  	
  	<location>/sistema/500.jsp</location>
  </error-page>
</web-app>
